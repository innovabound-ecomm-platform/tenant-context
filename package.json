{
  "name": "@innovabound-ecomm-platform/tenant-context",
  "version": "2.0.0",
  "description": "Tenant context and resolution middleware for multi-tenant e-commerce platform",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "restricted"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/innovabound-ecomm-platform/ecomm-repos.git",
    "directory": "packages/tenant-context"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@innovabound-ecomm-platform/platform-db": "workspace:^",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@innovabound-ecomm-platform/typescript-config": "workspace:^",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.3",
    "vitest": "^3.0.0"
  },
  "peerDependencies": {
    "express": "^4.21.0"
  }
}
