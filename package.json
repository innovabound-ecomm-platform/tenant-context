{
  "name": "@innovabound-ecomm-platform/tenant-context",
  "version": "1.0.0",
  "description": "Multi-tenant context resolution and enforcement for e-commerce platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "restricted"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/innovabound-ecomm-platform/tenant-context"
  },
  "scripts": {
    "build": "tsc",
    "check-types": "tsc --noEmit",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@innovabound-ecomm-platform/redis-client": "workspace:*",
    "jose": "^6.0.11"
  },
  "devDependencies": {
    "@innovabound-ecomm-platform/eslint-config": "workspace:*",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.1",
    "eslint": "^9.34.0",
    "express": "^5.1.0",
    "typescript": "^5.9.0",
    "vitest": "^3.2.3"
  },
  "peerDependencies": {
    "express": "^4.18.0 || ^5.0.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    }
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./express": {
      "import": "./dist/express/index.js",
      "types": "./dist/express/index.d.ts"
    }
  },
  "files": [
    "dist"
  ]
}
